{
    "manifest_version": 3,
    "name": "WhatsApp Translator",
    "version": "1.0",
    "description": "Translate and send WhatsApp messages in different languages.",
    "permissions": ["activeTab", "scripting", "https://web.whatsapp.com/*"],
    "content_scripts": [
      {
        "matches": ["https://web.whatsapp.com/*"],
        "js": ["app.js"]
      }
    ]
  }
  